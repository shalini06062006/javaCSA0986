2,javafx circle:

CODE:
import javafx.application.Application;
import javafx.scene.Scene;
import javafx.scene.layout.Pane;
import javafx.scene.paint.Color;
import javafx.scene.shape.Rectangle;
import javafx.stage.Stage;

public class SquareApp extends Application {

    @Override
    public void start(Stage primaryStage) {
        // Create a Pane to hold the square
        Pane pane = new Pane();

        // Create a square (Rectangle with equal width and height)
        Rectangle square = new Rectangle();
        square.setX(50);   // X position of the square
        square.setY(50);   // Y position of the square
        square.setWidth(100);  // Width of the square
        square.setHeight(100); // Height of the square
        square.setFill(Color.BLUE); // Fill color of the square

        // Add the square to the pane
        pane.getChildren().add(square);

        // Create a Scene and add the pane to it
        Scene scene = new Scene(pane, 200, 200);

        // Set the title of the Stage (window)
        primaryStage.setTitle("Square Example");

        // Set the scene for the Stage and display the window
        primaryStage.setScene(scene);
        primaryStage.show();
    }

    public static void main(String[] args) {
        // Launch the JavaFX application
        launch(args);
    }
}
